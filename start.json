{
  "daemon": true,
  "run": [{
    "method": "shell.run",
    "params": {
      "venv": "env",
      "path": "ComfyUI",
      "env": { "PYTORCH_MPS_HIGH_WATERMARK_RATIO": "0.0" },
      "message": "python main.py {{platform === 'darwin' ? '--force-fp16' : ''}}",
      "on": [{ "event": "/http:\/\/[0-9.:]+/", "done": true }]
    }
  }, {
    "method": "self.set",
    "params": {
      "session.json": {
        "url": "{{input.event[0]}}"
      }
    }
  }, {
    "method": "browser.open",
    "params": {
      "uri": "{{self.session.url}}"
    }
  }]
}
