{
  "requires": [{
    "type": "conda",
    "name": "ffmpeg",
    "args": "-c conda-forge"
  }],
  "run": [{
    "method": "shell.run",
    "params": {
      "message": "git clone https://github.com/comfyanonymous/ComfyUI.git"
    }
  }, {
    "method": "shell.run",
    "params": {
      "message": "git clone https://github.com/ltdrdata/ComfyUI-Manager",
      "path": "ComfyUI/custom_nodes"
    }
  }, {
    "method": "shell.run",
    "params": {
      "venv": "env",
      "path": "ComfyUI",
      "message": [
        "pip install -r requirements.txt",
        "pip install git+https://github.com/pytorch/pytorch@refs/pull/114183/head torchvision torchaudio"
      ]
    }
  }, {
    "method": "fs.download",
    "params": {
      "url": "https://huggingface.co/stabilityai/stable-diffusion-xl-base-1.0/resolve/main/sd_xl_base_1.0.safetensors",
      "path": "ComfyUI/models/checkpoints/sd_xl_base_1.0.safetensors"
    }
  }, {
    "method": "fs.download",
    "params": {
      "url": "https://huggingface.co/stabilityai/stable-diffusion-xl-refiner-1.0/resolve/main/sd_xl_refiner_1.0.safetensors",
      "path": "ComfyUI/models/checkpoints/sd_xl_refiner_1.0.safetensors"
    }
  }, {
    "method": "shell.run",
    "params": {
      "message": "mkdir workflows"
    }
  }, {
    "method": "shell.run",
    "params": {
      "message": [
        "git clone https://github.com/comfyanonymous/ComfyUI_examples",
        "git clone https://github.com/cocktailpeanut/comfymp4"
      ],
      "path": "workflows"
    }
  }, {
    "method": "input",
    "params": {
      "title": "Install Success",
      "description": "Go back to the dashboard and launch the app!"
    }
  }, {
    "method": "browser.open",
    "params": {
      "uri": "/?selected=ComfyUI"
    }
  }]
}
